flowchart Try-component {
  skewer main {
    unless has-children try-self/1
    run-each-child
    unless child-produced-output try-self/2
    > produced-output
  }
  skewer try-self {
    1:
    2:
    run-self
    unless self-produced-output no-output/3
    > produced-output
  }
  skewer no-output {
    3:
    send no-output _
    > finished
  }
  skewer produced-output {
    send produced-output _
    > finished
  }
  skewer finished {
    end
  }
}