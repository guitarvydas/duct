flowchart Try-component {
  start main
  skewer main {
    unless has-children try-self/1
    step-each-child
    unless child-wasActivated try-self/2
    > activated/0
  }
  skewer try-self {
    : try-self/1
    : try-self/2
    run-self
    unless self-wasActivated not-activated/3
    > activated/0
  }
  skewer not-activated {
    : not-activated/3
    >> no
    > finished/0
  }
  skewer activated {
    >> yes
    > finished/0
  }
  skewer finished {
    end
  }
}